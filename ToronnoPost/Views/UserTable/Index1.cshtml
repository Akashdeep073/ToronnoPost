@model List<WebShop.Models.AspNetUser>

@{

    ViewBag.Title = "Index";

}



<h2>User Details List</h2>



@using (Html.BeginForm("Index1", "UserTable", FormMethod.Post))

{

<table class="table">
    <tr>
        
        <td>
            Unique ID:
        </td>
        <td>
            Email ID:
        </td>
        <td>
            Password:
        </td>
        <td>
            Phone No:
        </td>
    </tr>
    @for (int i = 0; i < Model.Count; i++)

    {

    <tr>

        <td>

            @Html.Hidden("employees.Index", (@i + 10))

            @Html.TextBox("employees[" + (@i + 10) + "].ID",

             Model[i].Id, new { @readonly = "readonly" })

        </td>

        <td>

            @Html.TextBox("employees[" + (@i + 10) + "].Name",

             Model[i].Email)

        </td>

        <td>

            @Html.TextBox("employees[" + (@i + 10) + "].Gender",

             Model[i].PasswordHash)

        </td>

        <td>

            @Html.TextBox("employees[" + (@i + 10) + "].Company",

             Model[i].PhoneNumber)

        </td>
    </tr>
    }
    <tr>

        <td colspan="4">

            <input type="submit" value="Submit" />

        </td>

    </tr>

</table>

}